{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Secret": "super-secret-key",
    "ExpirationTimeInMinutes": 60,
    "Issuer": "RAMS",
    "Audience": "RAMS"
  },
  "Swagger": {
    "BearerOpenApiSecurityScheme": {
      "Scheme": "Bearer",
      "BearerFormat": "JWT",
      "Name": "Authorization",
      "Description": "Bearer Authentication with JWT Token"
    }
  },
  "ConnectionStrings": {
    "SqliteConnection": "Data Source=../Demo.db;Cache=Shared"
  }
}